{
    "version": 2,
    "builds": [
      {
        "src": "src/api/**/*",
        "use": "@vercel/node"
      },
      {
        "src": "build/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }
  